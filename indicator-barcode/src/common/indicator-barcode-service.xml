<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/">
  <interface name="com.canonical.indicator.network.backend.Manager">
    <method name="GetIcon">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol"
		  value="manager_proxy_get_icon"/>
      <arg name="name" type="s" direction="out"/>
    </method>
    <method name="RequestScan">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol"
		  value="manager_proxy_request_scan"/>
    </method>
    <method name="SetDebug">
      <arg type="i" direction="in"/>
      <annotation name="org.freedesktop.DBus.GLib.CSymbol"
		  value="manager_proxy_set_debug"/>
    </method>
    <signal name="IconChanged">
      <arg type="s"/>
    </signal>
    <signal name="AccessibleDescChanged">
      <arg type="s"/>
    </signal>
  </interface>
</node>

